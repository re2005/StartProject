{
    "env": {
        "browser": true,
        "jasmine": true
    },
    "globals": {
        "angular": true,
        "module": true,
        "inject": true,
        "_" : true,
        "Promise" : true
    },
    "rules": {
        "max-len" : 0,
        "vars-on-top" : 0,
        "no-underscore-dangle" : 0,
        "no-trailing-spaces": [
            "error",
            {
                "skipBlankLines": true
            }
        ],
        "indent": [
            "error",
            4
        ],
        "func-names" : 0
    }
}
